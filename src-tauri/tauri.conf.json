{
  "package": {
    "productName": "ScoBro Logbook",
    "version": "0.1.0"
  },
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:5173"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": false,
      "targets": "all",
      "identifier": "com.scobro.logbook"
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ScoBro Logbook",
        "width": 900,
        "height": 700,
        "label": "main"
      }
    ],
    "updater": {
      "active": true,
      "endpoints": [
        "https://releases.myapp.com/{{target}}/{{arch}}/{{current_version}}"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IHNpZ25hdHVyZSBmcm9tIHRhdXJpIHNlcnZpY2UKUlVUVjJMVmR5YjI5dFltbGhkR1Z5TURFd01nPT0K"
    }
  }
}